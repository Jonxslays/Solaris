{
    "help menu closed": {
        "header": "Help",
        "description": "The help menu was closed by the user."
    },
    "help menu timed out": {
        "header": "Help",
        "description": "The help menu timed out."
    },
    "help command": {
        "header": "Help",
        "description": "{description}",
        "fields": [
            ["Syntax (<required> • [optional])", "{syntax}", false],
            ["User requirements", "{user_reqs}", false],
            ["S4 requirements", "Read Messages\nSend Messages\n{bot_reqs}", false]
        ]
    },
    "about bot": {
        "header": "Information",
        "title": "About S4",
        "description": "Use `@S4 botinfo` for detailed statistics.",
        "fields": [
            ["Developer", "Created and copyright ©️ {info.developer} 2020.", false],
            ["Testers", "{info.testers}", false]
        ]
    },
    "support link": {
        "header": "Information",
        "title": "Join the support server",
        "description": "Click [here](https://discord.gg/c3b4cZs) to join the support server.",
        "fields": [
            ["Online / members", "{online_m:,} / {members:,}", true],
            ["Online / helpers", "{online_h:,} / {helpers:,}", true],
            ["Developer", "{status}", true]
        ]
    },
    "invite link": {
        "header": "Information",
        "title": "Invite S4 to your server",
        "description": "- To invite S4 with the Administrator permission, click [here]({al}).\n- To invite S4 with the minimum required permissions, click [here]({nal}) (you may need to grant S4 some extra permissions in order to use some modules).\n\nPlease note that S4 is currently pre-release.",
        "fields": [
            ["Servers", "{bot.guild_count}", true],
            ["Users", "{bot.user_count}", true],
            ["Default prefix", ">>", true]
        ]
    },
    "bot info": {
        "header": "Information",
        "title": "Bot information",
        "description": "S4 was developed by {owner.mention}. Use `@S4 about` for more information.",
        "fields": [
            ["Bot version", "{bot.version}", true],
            ["Python version", "{py_version}", true],
            ["discord.py version", "{dpy_version}", true],
            ["Uptime", "{uptime}", true],
            ["CPU time", "{cpu_time}", true],
            ["Memory usage", "{mem_usage:,.3f} / {mem_total:,.0f} ({mem_percent:.0f}%)", true],
            ["Servers", "{bot.guild_count:,}", true],
            ["Users", "{bot.user_count:,}", true],
            ["Commands", "{bot.command_count:,}", true],
            ["Python code", "{bot._python_lines:,}", true],
            ["JSON scripts", "{bot._json_lines:,}", true],
            ["SQL scripts", "{bot._sql_lines:,}", true],
            ["Database calls since uptime", "{db_calls:,} ({db_calls_ps:,.3f} per second)", true]
        ]
    },
    "user avatar": {
        "header": "Information",
        "description": "Displaying avatar for {name}."
    },
    "guild icon": {
        "header": "Information",
        "description": "Displaying icon for {target.name}."
    },
    "user info": {
        "header": "Information",
        "title": "User information for {target.name}",
        "description": "Showing reduced information as the given user is not in this server.",
        "fields": [
            ["ID", "{target.id}", true],
            ["Discriminator", "{target.discriminator}", true],
            ["Bot?", "{target.bot}", true],
            ["Created on", "{created_on}", true],
            ["Existed for", "{existed_for}", true],
            ["\u200b", "\u200b", true]
        ]
    },
    "member info": {
        "header": "Information",
        "title": "User information for {target.name}",
        "fields": [
            ["ID", "{target.id}", false],
            ["Discriminator", "{target.discriminator}", true],
            ["Bot?", "{target.bot}", true],
            ["Admin?", "{target.guild_permissions.administrator}", true],
            ["Created on", "{created_on}", true],
            ["Joined on", "{joined_on}", true],
            ["Boosted on", "{boosted_on}", true],
            ["Existed for", "{existed_for}", true],
            ["Member for", "{member_for}", true],
            ["Booster for", "{booster_for}", true],
            ["Status", "{status}", true],
            ["Activity type", "{activity_type}", true],
            ["Activity name", "{activity_name}", true],
            ["Nº of roles", "{no_of_roles:,}", true],
            ["Top role", "{target.top_role.mention}", true],
            ["Top role position", "{top_role_position} / {no_of_guild_roles:,}", true]
        ]
    },
    "guild info": {
        "header": "Information",
        "title": "Server information for {target.name}",
        "fields": [
            ["ID", "{target.id}", false],
            ["Owner", "{target.owner.mention}", true],
            ["Region", "{target.region}", true],
            ["Top role", "{top_role.mention}", true],
            ["Members", "{target.member_count:,}", true],
            ["Humans / bots", "{human_count:,} / {bot_count:,}", true],
            ["Bans", "{ban_count}", true],
            ["Roles", "{role_count:,}", true],
            ["Text channels", "{tc_count:,}", true],
            ["Voice channels", "{vc_count:,}", true],
            ["Invites", "{invite_count}", true],
            ["Emojis", "{emoji_count:,} / {emoji_limit:,}", true],
            ["Boosts", "{target.premium_subscription_count:,} (level {target.premium_tier})", true],
            ["Newest member", "{newest_member.mention}", true],
            ["Created on", "{created_on}", true],
            ["Existed for", "{existed_for}", true],
            ["Statuses", "🟢 {online_count:,} 🟠 {idle_count:,} 🔴 {dnd_count:,} ⚪ {offline_count:,}", false]
        ]
    }
}
